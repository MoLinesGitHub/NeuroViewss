{
	"mcpServers": {
		"task-master-ai": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"--package=task-master-ai",
				"task-master-ai"
			],
			"env": {
				"OPENAI_API_KEY": "${OPENAI_API_KEY}"
			}
		},
		"xcode-mcp": {
			"type": "stdio",
			"command": "node",
			"args": [
				"/Volumes/SSD/xCode_Projects/NeuroViews/mcps/xcode-mcp/dist/index.js"
			]
		},
		"swift-mcp": {
			"type": "stdio",
			"command": "node",
			"args": [
				"/Volumes/SSD/xCode_Projects/NeuroViews/mcps/swift-mcp/dist/index.js"
			]
		},
		"ios-simulator-mcp": {
			"type": "stdio",
			"command": "node",
			"args": [
				"/Volumes/SSD/xCode_Projects/NeuroViews/mcps/ios-simulator-mcp/dist/index.js"
			]
		},
		"github": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-github"
			],
			"env": {
				"GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
			}
		},
		"sequential-thinking": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			]
		},
		"memory": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory"
			]
		}
	}
}
